<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="dialog_loading" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="CSTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="colorPrimary">@color/cs_colorPrimary</item>
        <item name="colorPrimaryDark">@color/cs_colorPrimary</item>
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowBackground">@color/cs_colorPrimary</item>
    </style>
</resources>